@startuml
package controller {
    class LogStaffController {
        +manageTestLogistics()
    }
}

package repository {
    class StorageRepo {
        +storeTestLogisticsDetails()
    }

    class StorageRepo {
        +queryListVenues()
    }
}

package entity {
    class TestLogistics {
        +id: int
        +testId: int
        +details: String
    }

    class Venue {
        +id: int
        +name: String
        +location: String
    }
}

LogStaffController --> StorageRepo : uses
StorageRepo --> TestLogistics : stores
@enduml

