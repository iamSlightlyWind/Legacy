@startuml
package controller {
    class LogStaffController {
        +assignInterviewVenues()
    }
}

package repository {
    class VenueRepo {
        +storeInterviewVenueDetails()
    }
}

package entity {
    class Venue {
        +id: int
        +name: String
        +location: String
    }
}

LogStaffController --> VenueRepo : uses
VenueRepo --> Venue : stores
@enduml