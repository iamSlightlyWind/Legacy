@startuml
package controller {
    class LogStaffController {
        +assignInterviewVenues()
    }
}

package repository {
    class StorageRepo {
        +storeInterviewVenueDetails()
    }
}

package entity {
    class Venue {
        +id: int
        +name: String
        +location: String
    }
}

LogStaffController --> StorageRepo : uses
StorageRepo --> Venue : stores
@enduml