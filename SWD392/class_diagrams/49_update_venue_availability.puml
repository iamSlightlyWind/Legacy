@startuml
package controller {
    class LogStaffController {
        +updateVenueAvailability()
    }
}

package repository {
    class VenueRepo {
        +updateVenueAvailability()
    }
}

package entity {
    class Venue {
        +id: int
        +name: String
        +location: String
        +availability: String
    }
}

LogStaffController --> VenueRepo : uses
