@startuml
package controller {
    class StudentController {
        +fillApplicationForm()
    }
}

package repository {
    class FormRepo {
        +storeApplicationForm()
    }
}

package entity {
    class Form {
        +id: int
        +userId: int
        +formData: String
    }
}

StudentController --> FormRepo : uses
FormRepo --> Form : stores
@enduml