@startuml
package controller {
    class LogStaffController {
        +viewInterview()
    }
}

package repository {
    class InterviewRepo {
        +queryInterviewDetails()
    }
}

package service {
    class StorageService {
        +fetchInterviewFiles()
    }
}

package entity {
    class Interview {
        +id: int
        +date: Date
        +status: String
    }
}

