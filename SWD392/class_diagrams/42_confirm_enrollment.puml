@startuml
package controller {
    class StudentController {
        +confirmEnrollment()
    }
}

package repository {
    class EnrollmentRepo {
        +storeEnrollmentConfirmation()
    }
}

package entity {
    class Enrollment {
        +id: int
        +userId: int
        +status: String
        +date: Date
    }
}

StudentController --> EnrollmentRepo : uses
EnrollmentRepo --> Enrollment : stores
@enduml